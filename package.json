    "svelte-check": "^4.0.4",
    "svelte-hamburgers": "^4.2.1",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "uuid": "^10.0.0",
    "vite": "^5.4.8"
