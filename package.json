    "pdfmake": "^0.2.13"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.1.0",
    "@neoconfetti/svelte": "^2.2.1",
    "@picocss/pico": "^2.0.6",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/adapter-node": "^5.2.5",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.6.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/autosize": "^4.0.3",
    "@types/eslint": "^9.6.1",
