    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.8.5",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "uuid": "^10.0.0",
    "vite": "^5.4.1"
